package com.collectionss;

import java.util.ArrayList;
import java.util.List;

public class Array_List {
	
	public static void main(String[] args) {
		
		List<Integer> li = new ArrayList<Integer>();
		li.add(1);
		li.add(2);
		li.get(1); //2
		li.add(3);
		li.size();  //3
		System.out.println(li);
		
		
		
		
	
		}
}
