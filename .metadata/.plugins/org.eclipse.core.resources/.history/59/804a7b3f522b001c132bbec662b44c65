package com.string;

public class String_How {

	public static void main(String[] args) {

		String a = "java world";

		String b = "java world";

		String c;

		System.out.println(System.identityHashCode(a));
		System.out.println(System.identityHashCode(b));

		c = a + b;
		System.out.println(System.identityHashCode(c));
		
		
		
	}

}
